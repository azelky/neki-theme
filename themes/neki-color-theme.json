  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6B699D"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.block variable.other",
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#acb3e0"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
